<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/LightBrownColor</item>
        <item name="colorPrimaryDark">@color/LowLightBrownColor</item>
        <item name="colorAccent">@color/BlueColor</item>
    </style>

    <style name="NoActionBarTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/LightBrownColor</item>
        <item name="colorPrimaryDark">@color/LowLightBrownColor</item>
        <item name="colorAccent">@color/BlueColor</item>
    </style>



    <style name="SplashScreenNoActionBarTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/LightBrownColor</item>
        <item name="colorPrimaryDark">@color/BlackColor</item>
        <item name="colorAccent">@color/LowLightBrownColor</item>
    </style>


</resources>
